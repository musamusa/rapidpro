-for http_log in log.http_logs
  .mb-4
    -include "includes/http_log.haml" with log=http_log
-if log.errors
  -for err in log.errors
    .p-4.shadow.bg-white.rounded-lg.my-4
      .entry.flex
        %temba-icon.ml-2.mr-4(name="alert-triangle" style="--icon-color:rgb(var(--error-rgb))" size="1.5")
        .msg.flex-grow
          {{ err.message }}